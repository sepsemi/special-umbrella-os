interface(`googlechrome_domtrans',`
	gen_require(`
		type googlechrome_t, googlechrome_exec_t;
	')

	corecmd_search_bin($1)
	domtrans_pattern($1, googlechrome_exec_t, googlechrome_t)
')

interface(`googlechrome_run',`
	gen_require(`
		type googlechrome_t;
		attribute_role googlechrome_roles;
	')

	googlechrome_domtrans($1)
	roleattribute $2 googlechrome_roles;
')